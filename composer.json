{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "<=8.2",
    "ext-bcmath": "*",
    "ext-calendar": "*",
    "ext-curl": "*",
    "ext-igbinary": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-sockets": "*",
    "ext-zlib": "*",
    "genealabs/laravel-model-caching": "^11.0.1",
    "laravel/lumen-framework": "^11.0.0",
    "laravel/socialite": "^5.0.0",
    "pda/pheanstalk": "^6.0.0",
    "socialiteproviders/apple": "^5.5.0",
    "socialiteproviders/google": "^4.1.0",
    "symfony/mailer": "^7.0.0",
    "tymon/jwt-auth": "^2.1.0"
  },
  "require-dev": {
    "flipbox/lumen-generator": "^9.2.0",
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "laravel/homestead": "^15.0.0",
    "laravel/pint": "^1.0.0",
    "mockery/mockery": "^1.6.5",
    "phpstan/phpstan": "^1.0.0",
    "phpunit/phpunit": "^10.0.0",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.0.0"
  },
  "autoload": {
    "classmap": [
      "src/database/seeds",
      "src/database/factories"
    ],
    "files": [
      "src/core/_helpers.php"
    ],
    "psr-4": {
      "Modules\\": "src/modules/",
      "App\\": "src/core/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "src/tests/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('src/.env') || copy('src/.env.example', 'src/.env');\"",
      "@php -r \"file_exists('ex.Homestead.yaml') || copy('ex.Homestead.yaml', 'Homestead.yaml');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform-check": false,
    "platform": {
      "php": "8.2"
    },
    "github-oauth": {

    }
  },
  "provide": {
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-zlib": "*",
    "ext-redis": "*",
    "ext-pcntl": "*",
    "ext-swoole": "*",
    "ext-bcmath": "*",
    "ext-imagick": "*",
    "ext-openssl": "*",
    "ext-sockets": "*",
    "ext-igbinary": "*",
    "ext-mbstring": "*",
    "ext-calendar": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
